{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "no-multiple-empty-lines": ["error", { "max": 2 }],
        "quotes": ["error", "single"],
        "import/extensions": "off"    
    }
}
